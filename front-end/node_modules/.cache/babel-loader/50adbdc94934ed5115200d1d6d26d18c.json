","password","taLogado","rulesUsername","rulesPassword","methods","loginUser","then","console","catch"],"sourceRoot":"src/components","sources":["Login.vue"],"sourcesContent":["<template>\r\n    <v-container class=\"justify-center\" fill-height fluid>\r\n        <v-card class=\"pa-10\" elevation=\"2\">\r\n            <v-form ref=\"form\"  lazy-validation>\r\n                <p>Calendário de eventos</p>\r\n                <div>\r\n                    <v-text-field\r\n                    v-model=\"username\"\r\n                    label=\"Usuário\"\r\n                    :rules=\"rulesUsername\"\r\n                    hide-details=\"auto\"\r\n                    ></v-text-field>\r\n                    <v-text-field v-model=\"password\" :rules=\"rulesPassword\" label=\"Senha\"></v-text-field>\r\n                </div>\r\n                <v-btn\r\n                    block\r\n                    color=\"primary\"\r\n                    elevation=\"4\"\r\n                    v-on:click.prevent=\"loginUser\"\r\n                >Login</v-btn>\r\n                <v-btn\r\n                    block\r\n                    color=\"primary\"\r\n                    elevation=\"4\"\r\n                    v-on:click.prevent=\"profile\"\r\n                >Profile</v-btn>\r\n            </v-form>\r\n            {{taLogado}}\r\n        </v-card>\r\n    </v-container>\r\n</template>\r\n\r\n<script>\r\nimport axios from '../axios';\r\naxios.defaults.baseURL = 'http://localhost:8080';\r\n\r\nexport default {\r\n    // eslint-disable-next-line\r\n    name: 'Login',\r\n    data: () => ({\r\n        username: '',\r\n        password: '',\r\n        taLogado:false,\r\n        rulesUsername: [\r\n            value => !!value || 'Required.',\r\n        ],\r\n        rulesPassword: [\r\n            value => !!value || 'Required.',\r\n            value => (value && value.length >= 8) || 'Min 8 characters',\r\n        ],\r\n    }),\r\n    methods: {\r\n        loginUser() {\r\n            // if