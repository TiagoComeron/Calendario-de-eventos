rMensagem(text, cor) {\n      this.text = text;\n      this.corSnackbar = cor;\n      this.snackbar = true;\n    },\n    post(url, body, redirect, message, cor){\n      let res = null\n      axios.post(url, body, {headers: {'Authorization': ''}})\n      .then(response => {\n          // JSON responses are automatically parsed.\n          res = response.data\n          this.$router.push('/'+redirect)\n          if(message){\n            this.exibirMensagem(res.message, cor)\n          }\n      })\n      .catch(e => {\n          console.log(e)\n          if(e.response.status == 403){\n              this.exibirMensagem(this.posts.message, 'error')\n              this.$router.push('/')\n              return\n          }\n      })\n\n      return res\n    }\n  },\n  watch:{\n    '$router.currentRoute.path'(){\n      console.log(this)\n    }\n  }\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}